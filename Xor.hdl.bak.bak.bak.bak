/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a,out=abar);
    Not(in=b,out=bbar);
    And(a=abar,b=b,out=lhs);
    And(a=a,b=bbar,out=rhs);
    Or(a=lhs,b=rhs,out=out);
}